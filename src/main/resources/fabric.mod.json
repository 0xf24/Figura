{
  "schemaVersion": 1,
  "id": "figura",
  "version": "${version}",

  "name": "Figura",
  "description": "Figura is a client-side mod that allows players to change their player model and highly customize it, while also lettin other players see models in a safe environment",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/figura/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": [
      "net.blancworks.figura.FiguraMod"
    ]
  },
  "mixins": [
    "figura.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "another-mod": "*"
  }
}
